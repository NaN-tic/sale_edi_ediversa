<tryton>
    <data grouped="1">
        <record model="ir.message" id="msg_no_product">
            <field name="text">Cannot confirm sale without product "%(code)s".</field>
        </record>
        <record model="ir.message" id="msg_no_cancel_sale">
            <field name="text">Cannot cancel edi sale if sale with number "%(number)s is not cancelled"</field>
        </record>
        <record model="ir.message" id="msg_no_product_salable">
            <field name="text">Cannot confirm sale without product "%(code)s" is salable.</field>
        </record>
        <record model="ir.message" id="msg_to_many_sales">
            <field name="text">There are to many sales related to this EDI sale: %(sales)s</field>
        </record>
        <record model="ir.message" id="msg_cancel_sale_edi_ediversa">
            <field name="text">The EDI sales "%(edi_sales)s" will be cancelled.</field>
        </record>
        <record model="ir.message" id="msg_edi_sale_with_sale">
            <field name="text">The EDI sale %(edi_sale)s selected already has a sale (%(sale)s) related. That's not correct, must be unic.</field>
        </record>
    </data>
</tryton>
